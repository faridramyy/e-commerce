    />
    <style type="text/css">
      * {
        animation-duration: 0.001s !important;
        transition-duration: 0.001s !important;
      }
    </style>
  </head>
  <body
    class="category-page"
    data-aos-delay="0"
    data-aos-duration="600"
    data-aos-easing="ease-in-out"
  >
    <%- include("./partials/header.ejs") %>
    <main class="main">
      <!-- Page Title -->
      <div class="page-title light-background">
        <div
