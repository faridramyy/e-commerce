                      <li><a class="subcategory-link" href="#">Skincare</a></li>
                      <li><a class="subcategory-link" href="#">Makeup</a></li>
                      <li>
                        <a class="subcategory-link" href="#">Hair Care</a>
                      </li>
                      <li>
                        <a class="subcategory-link" href="#">Fragrances</a>
                      </li>
                    </ul>
                  </li>
                  <!-- Sports & Outdoors Category -->
                  <li class="category-item">
                    <div
                      aria-controls="categories-1-sports-subcategories"
                      aria-expanded="false"
                      class="d-flex justify-content-between align-items-center category-header collapsed"
                      data-bs-target="#categories-1-sports-subcategories"
                      data-bs-toggle="collapse"
                    >
                      <a class="category-link" href="javascript:void(0)"
                        >Sports &amp; Outdoors</a
                      >
                      <span class="category-toggle">
                        <i class="bi bi-chevron-down"></i>
                        <i class="bi bi-chevron-up"></i>
                      </span>
                    </div>
                    <ul
                      class="subcategory-list list-unstyled collapse ps-3 mt-2"
                      id="categories-1-sports-subcategories"
                    >
                      <li>
                        <a class="subcategory-link" href="#"
                          >Fitness Equipment</a
                        >
                      </li>
                      <li>
                        <a class="subcategory-link" href="#">Outdoor Gear</a>
                      </li>
                      <li>
                        <a class="subcategory-link" href="#">Sports Apparel</a>
                      </li>
                      <li>
                        <a class="subcategory-link" href="#">Team Sports</a>
                      </li>
                    </ul>
                  </li>
                  <!-- Books Category (no subcategories) -->
                  <li class="category-item">
                    <div
                      class="d-flex justify-content-between align-items-center category-header"
                    >
                      <a class="category-link" href="#">Books</a>
                    </div>
                  </li>
                  <!-- Toys & Games Category -->
                  <li class="category-item">
                    <div
                      aria-controls="categories-1-toys-subcategories"
                      aria-expanded="false"
                      class="d-flex justify-content-between align-items-center category-header collapsed"
                      data-bs-target="#categories-1-toys-subcategories"
                      data-bs-toggle="collapse"
                    >
                      <a class="category-link" href="javascript:void(0)"
                        >Toys &amp; Games</a
                      >
                      <span class="category-toggle">
                        <i class="bi bi-chevron-down"></i>
                        <i class="bi bi-chevron-up"></i>
                      </span>
                    </div>
                    <ul
                      class="subcategory-list list-unstyled collapse ps-3 mt-2"
                      id="categories-1-toys-subcategories"
                    >
                      <li>
                        <a class="subcategory-link" href="#">Board Games</a>
                      </li>
                      <li><a class="subcategory-link" href="#">Puzzles</a></li>
                      <li>
                        <a class="subcategory-link" href="#">Action Figures</a>
                      </li>
                      <li>
                        <a class="subcategory-link" href="#"
                          >Educational Toys</a
                        >
                      </li>
                    </ul>
                  </li>
                </ul>
              </div>
              <!--/Product Categories Widget -->
              <!-- Pricing Range Widget -->
              <div class="pricing-range-widget widget-item">
                <h3 class="widget-title">Price Range</h3>
                <div class="price-range-container">
                  <div class="current-range mb-3">
                    <span class="min-price">$0</span>
                    <span class="max-price float-end">$500</span>
                  </div>
                  <div class="range-slider">
                    <div class="slider-track"></div>
                    <div
                      class="slider-progress"
                      style="left: 0%; width: 50%"
                    ></div>
                    <input
                      class="min-range"
                      max="1000"
                      min="0"
                      step="10"
                      type="range"
                      value="0"
                    />
                    <input
                      class="max-range"
                      max="1000"
                      min="0"
                      step="10"
                      type="range"
                      value="500"
                    />
                  </div>
                  <div class="price-inputs mt-3">
                    <div class="row g-2">
                      <div class="col-6">
                        <div class="input-group input-group-sm">
                          <span class="input-group-text">$</span>
                          <input
                            class="form-control min-price-input"
                            max="1000"
                            min="0"
                            placeholder="Min"
                            step="10"
                            type="number"
                            value="0"
                          />
                        </div>
                      </div>
                      <div class="col-6">
                        <div class="input-group input-group-sm">
                          <span class="input-group-text">$</span>
                          <input
                            class="form-control max-price-input"
                            max="1000"
                            min="0"
                            placeholder="Max"
                            step="10"
                            type="number"
                            value="500"
                          />
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="filter-actions mt-3">
                    <button class="btn btn-sm btn-primary w-100" type="button">
                      Apply Filter
                    </button>
                  </div>
                </div>
              </div>
              <!--/Pricing Range Widget -->
              <!-- Brand Filter Widget -->
              <h3 class="brand-filter-widget widget-item">Filter by Brand</h3>
              <!--/Brand Filter Widget -->
              <!-- Color Filter Widget -->
              <div class="color-filter-widget widget-item">
                <h3 class="widget-title">Filter by Color</h3>
                <div class="color-filter-content">
                  <div class="color-options">
                    <div class="form-check color-option">
                      <input
                        class="form-check-input"
                        id="color-black"
                        type="checkbox"
                        value="black"
                      />
                      <label class="form-check-label" for="color-black">
                        <span
                          class="color-swatch"
                          style="background-color: #000000"
                          title="Black"
                        ></span>
                      </label>
                    </div>
                    <div class="form-check color-option">
                      <input
                        class="form-check-input"
                        id="color-white"
                        type="checkbox"
                        value="white"
                      />
                      <label class="form-check-label" for="color-white">
                        <span
                          class="color-swatch"
                          style="background-color: #ffffff"
                          title="White"
                        ></span>
                      </label>
                    </div>
                    <div class="form-check color-option">
                      <input
                        class="form-check-input"
                        id="color-red"
                        type="checkbox"
                        value="red"
                      />
                      <label class="form-check-label" for="color-red">
                        <span
                          class="color-swatch"
                          style="background-color: #e74c3c"
                          title="Red"
                        ></span>
                      </label>
                    </div>
                    <div class="form-check color-option">
                      <input
                        class="form-check-input"
                        id="color-blue"
                        type="checkbox"
                        value="blue"
                      />
                      <label class="form-check-label" for="color-blue">
                        <span
                          class="color-swatch"
                          style="background-color: #3498db"
                          title="Blue"
                        ></span>
                      </label>
                    </div>
                    <div class="form-check color-option">
                      <input
                        class="form-check-input"
                        id="color-green"
                        type="checkbox"
                        value="green"
                      />
                      <label class="form-check-label" for="color-green">
                        <span
                          class="color-swatch"
                          style="background-color: #2ecc71"
                          title="Green"
                        ></span>
                      </label>
                    </div>
                    <div class="form-check color-option">
                      <input
                        class="form-check-input"
                        id="color-yellow"
                        type="checkbox"
                        value="yellow"
                      />
                      <label class="form-check-label" for="color-yellow">
                        <span
                          class="color-swatch"
                          style="background-color: #f1c40f"
                          title="Yellow"
                        ></span>
                      </label>
                    </div>
                    <div class="form-check color-option">
                      <input
                        class="form-check-input"
                        id="color-purple"
                        type="checkbox"
                        value="purple"
                      />
                      <label class="form-check-label" for="color-purple">
                        <span
                          class="color-swatch"
                          style="background-color: #9b59b6"
                          title="Purple"
                        ></span>
                      </label>
                    </div>
                    <div class="form-check color-option">
                      <input
                        class="form-check-input"
                        id="color-orange"
                        type="checkbox"
                        value="orange"
                      />
                      <label class="form-check-label" for="color-orange">
                        <span
                          class="color-swatch"
                          style="background-color: #e67e22"
                          title="Orange"
                        ></span>
                      </label>
                    </div>
                    <div class="form-check color-option">
                      <input
                        class="form-check-input"
                        id="color-pink"
                        type="checkbox"
                        value="pink"
                      />
                      <label class="form-check-label" for="color-pink">
                        <span
                          class="color-swatch"
                          style="background-color: #fd79a8"
                          title="Pink"
                        ></span>
                      </label>
                    </div>
                    <div class="form-check color-option">
                      <input
                        class="form-check-input"
                        id="color-brown"
                        type="checkbox"
                        value="brown"
                      />
                      <label class="form-check-label" for="color-brown">
                        <span
                          class="color-swatch"
                          style="background-color: #795548"
                          title="Brown"
                        ></span>
                      </label>
                    </div>
                  </div>
                  <div class="filter-actions mt-3">
                    <button
                      class="btn btn-sm btn-outline-secondary"
                      type="button"
                    >
                      Clear All
                    </button>
                    <button class="btn btn-sm btn-primary" type="button">
                      Apply Filter
                    </button>
                  </div>
                </div>
              </div>
              <!--/Color Filter Widget -->
              <!-- Brand Filter Widget -->
              <div class="brand-filter-widget widget-item">
                <h3 class="widget-title">Filter by Brand</h3>
                <div class="brand-filter-content">
                  <div class="brand-search">
                    <input
                      class="form-control"
                      placeholder="Search brands..."
                      type="text"
                    />
                    <i class="bi bi-search"></i>
                  </div>
                  <div class="brand-list">
                    <div class="brand-item">
                      <div class="form-check">
                        <input
                          class="form-check-input"
                          id="brand1"
